{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=productionDebug",
    "android:dev": "react-native run-android --variant=developmentDebug --appIdSuffix=dev",
    "android:prod": "react-native run-android --variant=productionDebug",
    "android:stag": "react-native run-android --variant=stagingDebug --appIdSuffix=stag",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --scheme 'MyApp-dev'",
    "ios:prod": "react-native run-ios --scheme 'MyApp'",
    "ios:stag": "react-native run-ios --scheme 'MyApp-stag'",
    "lint": "eslint .",
    "lint:fix": "yarn eslint --fix",
    "start": "react-native start",
    "test": "jest",
    "type-check": "tsc",
    "test:report": "jest --config ./jest.config.js --coverage",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "pretty": "prettier --write ."
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "i18next": "^22.4.10",
    "react": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.71.4",
    "react-native-config": "^1.5.0",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.6.2",
    "react-native-reanimated": "3.0.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^3.20.0",
    "whatwg-fetch": "^3.6.2",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-navigation/devtools": "^6.0.18",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.182.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  }
}
